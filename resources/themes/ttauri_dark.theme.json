{
    "name": "default",
    "mode": "dark",

    // The colors values are either:
    // - integers are in sRGB
    // - floats are in linear-extended-sRGB
    // - string name of an color in the theme.

    // Bright colors, fitting theme.
    "blue"       : [ 10, 132, 255],
    "green"      : [ 48, 209,  88],
    "indigo"     : [ 94,  92, 230],
    "orange"     : [255, 159,  10],
    "pink"       : [255,  55,  95],
    "purple"     : [191,  90, 242],
    "red"        : [255,  69,  58],
    "teal"       : [100, 210, 255],
    "yellow"     : [255, 214,  10],

    "gray"       : [142, 142, 147],
    "gray2"      : [ 99,  99, 102],
    "gray3"      : [ 72,  72,  74],
    "gray4"      : [ 58,  58,  60],
    "gray5"      : [ 44,  44,  46],
    "gray6"      : [ 28,  28,  30],

    // Colors used by widgets, to show nesting inside containers,
    "fill-color": [
        [ 28,  28,  30],
        [ 44,  44,  46],
        [ 58,  58,  60],
        [ 72,  72,  74]
    ],
    
    // Colors used by widgets for internal elements and disabled icons.
    "border-color": [
        [ 72,  72,  74],
        [ 99,  99, 102],
        [142, 142, 147],
        [174, 174, 178]
    ],

    // Color for textual or primary elements like icons.
    "foreground-color":       [250, 250, 255],

    // Accent color used by widgets, to show icons, hover and keyboard focus
    "accent-color":           [ 10, 132, 255],
    "text-select-color":      [ 10, 132, 255],
    "cursor-color":           [250, 250, 255],
    "incomplete-glyph-color": [250, 250,  10],

    "small-label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 10.0,
        "color": "foreground"
    },

    "label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 12.0,
        "color": "foreground"
    },
    "warning-label-style": {
        "family": "Times New Roman",
        "weight": "Normal",
        "size": 12.0,
        "color": "foreground"
    },
    "error-label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 12.0,
        "color": [255, 59, 48]
    },
    "help-label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 12.0,
        "color": [88, 86, 214]
    },
    "placeholder-label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 12.0,
        "color": [142, 142, 147]
    },
    "link-label-style": {
        "family": "Arial",
        "weight": "Normal",
        "size": 12.0,
        "color": [0, 122, 255]
    }
}
